{
  "name": "webpack2-boilerplate",
  "version": "1.0.0",
  "description": "Webpack2 Boilerplate",
  "main": "entry.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "NODE_ENV=test webpack ./src/app/entry.js ./dist/app.bundle.min.js",
    "build:dev": "NODE_ENV=development webpack ./src/app/entry.js ./dist/app.bundle.min.js",
    "build:prod": "NODE_ENV=production webpack -p ./src/app/entry.js ./dist/app.bundle.min.js",
    "serve": "webpack-dev-server ./src/app/entry.js ./dist/app.bundle.min.js --inline --progress"
  },
  "author": "Charles Brown-Roberts",
  "license": "MIT",
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.15",
    "coffee-loader": "^0.7.2",
    "config": "^1.25.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-env-loader": "^0.1.1"
  }
}
